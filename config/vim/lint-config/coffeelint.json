{
  "arrow_spacing": {
    "level": "warn"
  },
  "braces_spacing": {
    "level": "warn"
  },
  "no_trailing_whitespace": {
    "allowed_in_empty_lines": false
  },
  "max_line_length": {
    "level": "ignore"
  },
  "line_endings": {
    "level": "warn"
  },
  "indentation": {
    "value": 4
  },
  "colon_assignment_spacing": {
    "level": "warn",
    "spacing": {
      "left": 0,
      "right": 1
    }
  },
  "no_empty_param_list": {
    "level": "warn"
  },
  "no_stand_alone_at": {
    "level": "warn"
  },
  "space_operators": {
    "level": "warn"
  },
  "empty_constructor_needs_parens": {
    "level": "warn"
  },
  "cyclomatic_complexity": {
    "level": "warn"
  },
  "newlines_after_classes": {
    "level": "warn"
  },
  "no_unnecessary_double_quotes": {
    "level": "warn"
  },
  "no_interpolation_in_single_quotes": {
    "level": "warn"
  },
  "no_empty_functions": {
    "level": "warn"
  },
  "prefer_english_operator": {
    "level": "warn"
  },
  "spacing_after_comma": {
    "level": "warn"
  },
  "eol_last": {
    "level": "warn"
  }
}
